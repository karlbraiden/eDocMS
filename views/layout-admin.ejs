<body>
  <!-- Left Panel -->
  <aside id="left-panel" class="left-panel">
    <nav class="navbar navbar-expand-sm navbar-default">

      <div class="navbar-header">
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#main-menu" aria-controls="main-menu" aria-expanded="false" aria-label="Toggle navigation">
          <i class="fa fa-bars"></i>
        </button>
        <a class="navbar-brand" href="./"><img src="/images/logo.png" alt="Logo"></a>
        <a class="navbar-brand hidden" href="./"><img src="/images/logo2.png" alt="Logo"></a>
      </div>

      <div id="main-menu" class="main-menu collapse navbar-collapse">
        <ul class="nav navbar-nav">
          <h3 class="menu-title">Accounts</h3>
          <li>
            <a id="menuReg" href="/kalikot"> <i class="menu-icon fa fa-user"></i>Register</a>
          </li>
          <li>
            <a id="menuViewEdit" href="/viewusers"> <i class="menu-icon fa fa-table"></i>View/Edit/Del</a>
          </li>
          <h3 class="menu-title">Server Config</h3>
          <li>
            <a id="menuDrive" href="/configserve/drive"> <i class="menu-icon fa fa-tasks"></i>Drive Path</a>
          </li>
          <li class="menu-item-has-children dropdown">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><i class="menu-icon fa  fa-list-ol"></i>Lists</a>
            <ul class="sub-menu children dropdown-menu">
              <li><i class="menu-icon fa fa-users"></i><a id="menuBranch" href="/configserve/branch">Branches/Groups</a></li>
              <li><i class="menu-icon fa fa-sitemap"></i><a id="menuClass" href="/configserve/class">Classification</a></li>
            </ul>
          </li>
          <h3 class="menu-title">Communications</h3>
          <li class="menu-item-has-children dropdown">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><i class="menu-icon fa fa-laptop"></i>Monitoring</a>
            <ul class="sub-menu children dropdown-menu">
              <li><i class="menu-icon fa fa-bar-chart-o"></i><a href="/chartmonitor">Chart</a></li>
              <li><i class="menu-icon fa fa-table"></i><a href="/tablemonitor">Table</a></li>
            </ul>
          </li>


        </ul>
      </div><!-- /.navbar-collapse -->
    </nav>
  </aside><!-- /#left-panel -->

  <!-- Right Panel -->
  <div id="right-panel" class="right-panel">

    <!-- Header-->
    <header id="header" class="header">

      <div class="header-menu">

        <div class="col-sm-12">
          <a id="menuToggle" class="menutoggle pull-left"><i class="fa fa fa-tasks"></i></a>
          <div class="header-left">


            <form name="formroute" id="formroute">

            </form>

            <div class="header-right">

              <div class="user-area dropdown float-right">
                <a id= 'userProf' href="#" class="dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                  <img id="pixID" class="user-avatar rounded-circle" src="/images/nelson.jpg" alt="User Avatar">
                  <span class="count"><%= mailfiles.length %></span>
                </a>

                <div class="user-menu dropdown-menu">
                  <li><a class="nav-link" href="/myprofile"><i class="fa fa-user"></i> My Profile</a></li>
                  <li class="dropdown-submenu">
                    <a class="dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" href="#"><i class="fa fa-envelope"></i> Mail <span class="count"><%= mailfiles.length %></span></a>
                    <ul id='mailnoti'class="dropdown-menu" style="right:300px!important;width:160px!important;">
                      <% mailfiles.forEach (function (file){
                        var fileArr = file.split('/'); filename = fileArr[fileArr.length - 1].substring(0,12); %>
                        <li><button type='button' onclick="delNotiFile('<%= file %>')" class='btn btn-danger btn-sm fa fa-times' href="#"></button><button button type='button' class="btn btn-link btn-sm" onclick="openDisFile('<%= file %>')" href="#"><%= filename %></button></li>
                        <% }); %>
                      </ul>
                    </li>
                    <li><a id="disCheckDrive" class="nav-link"  href="#"><i class="fa fa-cog"></i> Check map drive</a></li>
                    <li><a id="disLogout" class="nav-link" href="#"><i class="fa fa-power-off"></i> Logout</a></li>
                  </div>
                </div>



              </div>
            </div>

          </header><!-- /header -->
          <!-- Layout for Content-->
          <%-body%>
        </div><!-- /#right-panel -->
      <!-- Modal Dialog Box -->
        <button id='mstrtoggleDialog' style='display:none' type="button" class="btn btn-secondary mb-1" data-toggle="modal" data-target="#mstrstaticModal"> </button>
        <div  class="modal fade" id="mstrstaticModal" tabindex="-1" role="dialog" aria-labelledby="staticModalLabel" aria-hidden="true" data-backdrop="static">
          <div class="modal-dialog modal-sm"  role="document">
            <div class="modal-content" style='min-width:350px;' >
              <div class="modal-header">
                <h5 class="modal-title">Information</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                  <span aria-hidden="true">&times;</span>
                </button>
              </div>
              <div id= 'mstrmodDisp' class="modal-body">
              </div>
              <div class="modal-footer">
                <button id='butInfoOk' type="button" class="btn btn-secondary" data-dismiss="modal">Ok</button>
              </div>
            </div>
          </div>
        </div>
        <!-- Modal Dialog Box -->


        <!-- Password Modal Dialog Box -->
        <button id='passwtoggleDialog' style='display:none' type="button" class="btn btn-secondary mb-1" data-toggle="modal" data-target="#passwstaticModal"> </button>
        <div  class="modal fade" id="passwstaticModal" tabindex="-1" role="dialog" aria-labelledby="staticModalLabel" aria-hidden="true" data-backdrop="static">
          <div class="modal-dialog modal-sm"  role="document">
            <div class="modal-content" style='min-width:350px;' >
              <div class="modal-header">
                <h5 class="modal-title" id="passwstaticModalLabel">Confirmation</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                  <span aria-hidden="true">&times;</span>
                </button>
              </div>
              <div id= 'passwmodDisp' class="modal-body">
                <p id='lblResult' class='h4'></p>
                <p class='h6'>Re-enter your password for validation:<input type='password' id='passwmodPass' class='input-sm form-control-sm form-control'></p>
              </div>
              <div class="modal-footer">
                <button id='passwmodCancel' type="button" class="btn btn-secondary" data-dismiss="modal">Cancel</button>
                <button id='passwbutConfirm' type="button" class="btn btn-primary">Confirm</button>
              </div>
            </div>
          </div>
        </div>
        <!-- Password Modal Dialog Box -->

        <!-- Right Panel -->

        <script src="/vendors/jquery/dist/jquery.min.js"></script>
        <script src="/vendors/popper.js/dist/umd/popper.min.js"></script>
